    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug EXPLO (intox_2D)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/x86_64-debug/bin/explo_intox_2D",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/x86_64-debug/bin",
            "environment": [
                {
                    "name": "LD_LIBRARY_PATH",
                    "value": "${workspaceFolder}/build/x86_64-debug/lib:${env:LD_LIBRARY_PATH}"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build-x86_64-debug"
        },
